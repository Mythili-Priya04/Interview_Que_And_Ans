//AS operator is used to type safe  CONVERSION between  reference type or nullable type

//EX code:
class njhgj
{
    static void Main(string[] args)
    {
        object obj = "fthyj";
        string s = obj as string;

        Console.WriteLine(s);//output : fthyj
    }
